{
  "name": "dpe-labels",
  "private": true,
  "version": "0.0.0",
  "author": "Sam Lemaresquier <contact@samlem.com> (samlem.com)",
  "description": "Petit plugin JS pour la génération et l'affichage de diagnostics de performance énergétique (DPE) sur le web. Léger, responsive, flexible.",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twekz/dpe-labels"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "browser": "./dist/dpe-labels.umd.cjs",
  "module": "./dist/dpe-labels.js",
  "exports": {
    ".": {
      "types": "./dist/dpe-labels.d.ts",
      "import": "./dist/dpe-labels.js",
      "require": "./dist/dpe-labels.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "git-commit": "cz",
    "lint": "eslint --fix",
    "prepare": "husky",
    "preview": "vite preview",
    "test:dev": "vitest",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@types/lodash": "^4.17.7",
    "@vitest/coverage-v8": "^2.0.4",
    "commitizen": "^4.3.0",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "inquirer": "^9.3.6",
    "lint-staged": "^15.2.7",
    "lodash": "^4.17.21",
    "neostandard": "^0.11.1",
    "sass": "^1.77.8",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^4.0.2",
    "vitest": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
